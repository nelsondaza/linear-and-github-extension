{
  "name": "@repo/vitest-config",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --cache --cache-location './dist/.eslintcache' --ext .js,.jsx,.ts,.tsx --ignore-path ../../.gitignore",
    "lint:files": "eslint --cache --cache-location './dist/.eslintcache' --ext .js,.jsx,.ts,.tsx --ignore-path ../../.gitignore --max-warnings 0",
    "lint:tsc": "tsc --noEmit",
    "pretty": "prettier --write --ignore-path ../../.gitignore . && npm run lint --fix"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@testing-library/jest-dom": "^6.6.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "vitest": "^2.1.4"
  }
}
